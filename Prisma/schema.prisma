generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserData {
  id    String   @id @map("_id") @default(auto()) @db.ObjectId
  UserId String  @unique
  name  String   @unique
  image String?  // nullable
  order Int
}

